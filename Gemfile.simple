source 'https://rubygems.org'

# Core Jekyll without problematic dependencies
gem 'jekyll', '~> 4.0'
gem 'jekyll-feed'
gem 'jekyll-sitemap'
gem 'jekyll-redirect-from'
gem 'jemoji'
gem 'webrick', '~> 1.8'

# Use kramdown instead of commonmarker (no native extensions)
gem 'kramdown'
gem 'kramdown-parser-gfm'
